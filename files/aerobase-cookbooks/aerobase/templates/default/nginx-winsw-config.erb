<configuration>
  
  <!-- ID of the service. It should be unique across the Windows system-->
  <id>Aerobase-Gateway-Server</id>
  <!-- Display name of the service -->
  <name><%= @srv_label %> Gateway</name>
  <!-- Service description -->
  <description><%= @srv_label %> Gateway (Powered by Aerobase)</description>
  
  <!-- Path to the executable, which should be started -->
  <executable><%= @nginx_dir %>\nginx.exe</executable>
  <% if @delayed_start %>
  <delayedAutoStart/>
  <% end %>
  
</configuration>
