<configuration>
  
  <!-- ID of the service. It should be unique accross the Windows system-->
  <id><%= @srv_label %>-Web-Server</id>
  <!-- Display name of the service -->
  <name><%= @srv_label %> Web-Server (powered by NGINX)</name>
  <!-- Service description -->
  <description><%= @srv_label %> Web-Server (powered by NGINX)</description>
  
  <!-- Path to the executable, which should be started -->
  <executable><%= @nginx_dir %>\nginx.exe</executable>

</configuration>
